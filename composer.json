{
    "name": "iamthewit/stock-exchange",
    "authors": [
        {
            "name": "Ben Cross",
            "email": "bencross86@gmail.com"
        }
    ],
    "description": "",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "prooph/pdo-event-store": "^1.13",
        "ramsey/uuid": "^4.1",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.2.*",
        "symfony/messenger": "5.2.*",
        "symfony/yaml": "5.2.*"
    },
    "require-dev": {
        "kint-php/kint": "^3.3",
        "phpunit/phpunit": "^9.5",
        "qossmic/deptrac-shim": "^0.13.0",
        "symfony/browser-kit": "^5.2",
        "symfony/css-selector": "^5.2",
        "symfony/phpunit-bridge": "^5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "StockExchange\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StockExchange\\Tests\\": "tests"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpunit-cc": "XDEBUG_MODE=coverage ./vendor/bin/phpunit --coverage-html ./code-coverage"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
